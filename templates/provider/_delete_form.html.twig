<form method="post" action="{{ path('provider_delete', {'id': provider.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce fournisseur?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ provider.id) }}">
    <button class="btn">Supprimer</button>
</form>
